int main()
{
      int n,i;
      int a=1,b=1,c=0;
      printf("请输入数字:");
      scanf("%d",&n);
      if(n>1)
      {
           for(i=1;i<=n;i++)
           {
               a=b;
               b=c;
               c=a+b;
           }
      }
         else
        c=1;
     printf("该数的斐波那契数列值是:%d",c);
      return 0;
}
